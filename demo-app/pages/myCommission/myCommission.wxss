/* component/myCommission/myCommission.wxss */

.top {
  position: relative;
  width: 100%;
  height: 340rpx;
}

.top .top_content {
  position: absolute;
  padding: 20rpx;
  width: 100%;
  top: 0;
  left: 0;
  box-sizing: border-box;
  color: #fff;
}

