<!-- <view class='mask' wx:if="{{!warrantMask}}" bindtap="warrantEven"> -->
<view class='mask' wx:if="{{false}}" bindtap="warrantEven" hidden='{{showAccount}}'>
	<view class='findAccount'>
		<view class='findTitle'>
			<image src='https://img.hmeshop.cn/Common/tubiao@2x.png' style="width:130rpx;height:130rpx;"></image>
			<text>发现其他关联账号</text>

		</view>
		<text class='dashed'></text>
		<view class='headportrait'>
			<image src="https://img.hmeshop.cn/Common/touxiang_01@2x.png" style="width:130rpx;height:130rpx;"></image>
			<image src="https://img.hmeshop.cn/Common/touxiang_02@2x.png" style="width:130rpx;height:130rpx;"></image>
			<image src="https://img.hmeshop.cn/Common/touxiang_03@2x.png" style="width:130rpx;height:130rpx;"></image>
		</view>
		<view class='headportrait'>
			<text>微信：DFD2554</text>
			<text>小程序：DFD2...</text>
			<text>APP：DFD2554</text>
		</view>
		<view class='textscript'>
			<text>
      选择下列其中一个账号作为主账号，合并后以该账号作为基本信息（例如：账号推荐人信息、推荐人信息等），其余账号相关信息（例如：订单、佣金等）将合并到当前账号上，此操作不可逆。
      </text>
		</view>
		<button class='btnyes' bindtap='sure'>确定</button>
		<text class='btnno' bindtap='later'>以后再说</text>
	</view>

</view>


<!-- 直播入口广告 -->
<block wx:if="{{ !userData.IsLiveAnchor && !showlogin }}">
	<view class="live-block">
		<view class="live-container">
			<!-- 背景 -->
			<image class="live-bg" src="https://img.hmeshop.cn/Common/bg_o2@2x.png"></image>
			<!-- 主体 -->
			<view class="list-block-left">
				<view class="text-content">
					<view class="b-text">申请成为主播，成为主播</view>
					<view class="s-text">成为下一个带货王！</view>
				</view>
			</view>
			<view class="list-block-right">
				<navigator url="/pages/joinNest/joinNest?parameter=/custom/beAnchor" hover-class="navigator-hover" open-type="navigate">
					<button class="jump-btn">申请主播</button>
				</navigator>
			</view>
		</view>
	</view>
</block>

