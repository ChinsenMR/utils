				<image src='/images/icon_10_s@2x.png'></image>
			</view>
			<view>申请开播</view>
		</navigator>
		<navigator class='tabli' url='/pages/anchorList/anchorList' hover-class="none">
			<view class='img'>
				<image src='/images/icon_02_z@2x.png'></image>
			</view>
			<view>直播列表</view>
		</navigator>
		<navigator class='tabli' url='/pages/anchorAward/anchorAward' hover-class="none">
