  border-color: #FF3E76FF!important;

  /* 自定义样式.... */
}
.act-wrap {
  height: 78rpx;
  background: rgba(248, 248, 248, 1);
  border-radius: 44rpx;
  padding-left: 35rpx;
  /* margin-top: 220rpx; */
}
.goods-list{
  height: 650rpx;
  overflow: scroll;
}
.goods-list .goods-item .avatar {
  width: 142rpx;
  height: 142rpx;
  background-size: cover;
  background-position: center center;
  background-repeat: no-repeat;
}
.goods-list .goods-item{
  padding: 24rpx 20rpx;
  margin-bottom: 11rpx;
  background:rgba(249,249,249,1);
}
.goods-list .goods-item .content .name {
  font-size: 28rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: rgba(51, 51, 51, 1);
}

.goods-list .goods-item .content .price {
  font-size: 26rpx;
  font-family: PingFang-SC-Bold, PingFang-SC;
  font-weight: bold;
  color: rgba(74, 74, 74, 1);
}