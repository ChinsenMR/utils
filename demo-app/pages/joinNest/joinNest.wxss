/* pages/joinNest/joinNest.wxss */
@import "../../wxParse/wxParse.wxss";
/* 商品列表 */
.list ul {
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  margin: 20px auto;
  width: 95%;
  background-color: #f9f9f9;
}
.list ul li {
  width: 48%;
  height: 307rpx;
  box-sizing: border-box;
}
.list ul li image {
  width: 100%;
  height: 180rpx;
}
.list ul li .title {
  font-size: 26rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin: 5rpx 0 5rpx 0;
}
.list ul li .price {
  font-size: 32rpx;
  color: #ff5f00;
}
.list ul li .price text {
  font-size: 24rpx;
  color: #929292;
  margin-left: 5rpx;
  text-decoration: line-through;
}

.richtext{
  background-color: white;
  padding: 20rpx;
}



