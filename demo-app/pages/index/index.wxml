<block wx:for="{{indexData}}" wx:key="{{index}}">
  <!-- 富文本 -->
  <block wx:if="{{item.type == 1}}">
    <view class='richtext'>
      <template is="wxParse" data="{{wxParseData:richtext.nodes}}" />
    </view>
  </block>

  <!-- 文本标题 -->
  <block wx:if="{{item.type == 2}}">
    <headings headingsUrls="{{item.content}}" />
  </block>

  <!-- 商品 -->
  <block wx:if="{{item.type == 4}}">
    <product-list productlist="{{item}}" />
  </block>

  <!-- 商品列表 -->
  <block wx:if="{{item.type == 5}}">
    <goods-List goodList="{{item}}" />
  </block>

  <!-- 搜索 -->
  <block wx:if="{{item.type == 6}}">
    <search/>
  </block>

  <!-- 文本导航 -->
  <block wx:if="{{item.type == 7}}">
    <text-Navigation textNavUrls="{{item}}" />
  </block>

  <!-- 图片导航 -->
  <block wx:if="{{item.type == 8}}">
    <sort-Navigation sortNavUrls="{{item}}" />
  </block>

  <!-- 图片广告 -->
  <block wx:if="{{item.type == 9}}">
    <ad-pic adpicUrls="{{item}}" />
  </block>

  <!-- 分割线 -->
  <block wx:if="{{item.type == 10}}">
    <cutting-Line cuttlineUrls="{{item.content}}" />
  </block>

  <!-- 辅助空白 -->
  <block wx:if="{{item.type == 11}}">
    <view style='height:{{item.content.height}}px'></view>
  </block>

  <!-- 顶部菜单 -->
  <block wx:if="{{item.type == 12}}">
    <top-Menu topMenuUrls="{{item}}" />
  </block>

  <!-- 橱窗 -->
  <block wx:if="{{item.type == 13}}">
    <show-Case showCaseList="{{item}}" />
  </block>

  <!-- 视频 -->
  <block wx:if="{{item.type == 14}}">
    <video style='width:100%; margin: 20rpx auto;' objectFit="fill" src="{{item.content.website2}}"></video>
  </block>
</block>
<btn-to-top show="{{showToTop}}" />


<banner-dialog id="dialog" noticeData="{{noticeData}}" show="{{showWindow}}" bind:closeWindow="closeWindow" />
<!-- 直播入口广告 -->
<block wx:if="{{noticeSwiperForm.background.length > 0 || liveSwiperForm.background.length > 0 }}">
  <view class="live-block">
    <view class="live-container">
      <!-- 背景 -->
      <image class="live-bg" src="https://img.hmeshop.cn/Common/bg@2x.png"></image>
      <!-- 主体 -->
      <view class="list-block-left">
        <view class="custom-swiper-box" wx:if="{{liveSwiperForm.background.length > 0}}">
          <image src="/images/icon_zhibo@2x.png"></image>
          <swiper class="custom-swiper" indicator-dots="{{liveSwiperForm.indicatorDots}}" autoplay="{{liveSwiperForm.autoplay}}" interval="{{liveSwiperForm.interval}}" duration="{{liveSwiperForm.duration}}" vertical="{{true}}" indicator-dots="{{false}}">
            <block wx:for="{{liveSwiperForm.background}}" wx:key="*this">
              <swiper-item>
                <view class="swiper-item {{item}}">
                  {{item.LiveAnchor}}
                  <text style="color: rgb(248,231,28)">正在直播</text>
                  {{item.LiveTitle}}
                </view>
              </swiper-item>
            </block>
          </swiper>
        </view>
        <view class="custom-swiper-box" wx:if="{{noticeSwiperForm.background.length > 0}}">
          <image src="/images/icon_book@2x.png"></image>
          <swiper class="custom-swiper" indicator-dots="{{noticeSwiperForm.indicatorDots}}" autoplay="{{noticeSwiperForm.autoplay}}" interval="{{noticeSwiperForm.interval}}" duration="{{noticeSwiperForm.duration}}" vertical="{{true}}" indicator-dots="{{false}}">
            <block wx:for="{{noticeSwiperForm.background}}" wx:key="*this">
              <swiper-item>
                <view class="swiper-item {{item}}">{{item.LiveAnchor}}{{item.LiveTitle}}</view>
              </swiper-item>
            </block>
          </swiper>
        </view>
      </view>
      <view class="list-block-right">
        <navigator url="/pages/liveLobby/liveLobby" hover-class="navigator-hover" open-type="navigate">
          <button class="jump-btn">
            <image src="/images/icon_play2@2.png"></image>
            看直播
          </button>
        </navigator>
      </view>
    </view>
  </view>
</block>